VERSION 1

NODE endpoint
SQL >
%
SELECT
    id,
    channelId,
    time,
    event,
    content,
    metadata
FROM events__v1

WHERE
    teamId = {{ String(teamId, required=True)}} AND
    {% if defined(channelId) %}
        channelId = {{ String(channelId) }} AND
    {% end %}
    time >= fromUnixTimestamp64Milli({{ Int64(since, required=True) }})
ORDER BY time DESC